<configuration>
  <!-- Fill in the values below-->
  <property>
    <name>fs.contract.test.fs.oraclebmc</name>
    <!-- Ex: oraclebmc://bucket@namespace  -->
    <!-- 'bucket' is the name of the ObjectStorage bucket -->
    <!-- 'namespace' is currently the tenancy name -->
    <value></value>
  </property>

  <property>
    <name>fs.oraclebmc.client.hostname</name>
    <!-- Change to a different endpoint if desired -->
    <value>https://objectstorage.us-phoenix-1.oraclecloud.com</value>
  </property>

  <property>
    <name>fs.oraclebmc.client.auth.tenantId</name>
    <value></value>
  </property>
  
  <property>
    <name>fs.oraclebmc.client.auth.userId</name>
    <value></value>
  </property>
  
  <property>
    <name>fs.oraclebmc.client.auth.fingerprint</name>
    <value></value>
  </property>

  <property>
    <name>fs.oraclebmc.client.auth.pemfilepath</name>
    <!-- Ex: /Absolute/path/to/private_key.pem</value> -->
    <value></value>
  </property>
  <property>
    <name>fs.oraclebmc.client.auth.passphrase</name>
    <value>YOU_KNOW_THIS</value>
  </property>
</configuration>
